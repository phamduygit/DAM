@model List<string>

@{
  ViewData["Title"] = "Table List";
}
<h1>@ViewData["Title"]</h1>

<div class="list-group">
  @foreach (var item in Model)
  {
    <a class="list-group-item list-group-item-action" onclick="location.href = '@(Url.Action("Handle", "Home", new { id = item }))'">@item</a>
  }
</div>
